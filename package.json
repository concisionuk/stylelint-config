{
  "name": "stylelint-config-concision",
  "version": "0.0.7",
  "description": "Standard shareable stylelint config for Concision projects",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "standard"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/concisionuk/stylelint-config-concision.git"
  },
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "dependencies": {
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-rational-order": "0.1.2",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-recommended-scss": "4.3.0",
    "stylelint-order": "4.1.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "3.19.0",
    "stylelint-webpack-plugin": "2.2.2"
  },
  "devDependencies": {
    "eslint": "7.30.0",
    "eslint-config-stylelint": "13.1.1",
    "jest": "25.1.0",
    "jest-preset-stylelint": "4.1.1",
    "npm-run-all": "4.1.5",
    "npmpub": "5.0.0",
    "remark-cli": "7.0.1",
    "remark-preset-lint-consistent": "2.0.4",
    "remark-preset-lint-recommended": "3.0.4"
  },
  "peerDependencies": {
    "stylelint": "^11.0.0 || ^12.0.0 || ^13.0.0"
  },
  "scripts": {
    "dry-release": "npmpub --dry --verbose",
    "lint:js": "eslint . --ignore-path .gitignore",
    "lint:md": "remark . --quiet --frail",
    "lint": "npm-run-all --parallel lint:*",
    "pretest": "npm run lint",
    "release": "npmpub",
    "test": "jest",
    "watch": "jest --watch"
  },
  "eslintConfig": {
    "extends": [
      "stylelint"
    ]
  },
  "jest": {
    "preset": "jest-preset-stylelint"
  },
  "remarkConfig": {
    "plugins": [
      "preset-lint-recommended",
      "preset-lint-consistent"
    ]
  },
  "resolutions": {
    "stylelint": "13.13.1",
    "minimist": ">=1.2.2",
    "acorn": ">=7.1.1"
  }
}
